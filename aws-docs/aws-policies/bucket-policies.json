{
  "comment": "Permissão para pegar objeto no bucket qualquer usuário pois o principal esta como *",
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Statement1",
      "Principal": "*",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": ["arn:aws:s3:::olimpiada-senai-sabado/*"]
    }
  ]
}